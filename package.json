{
  "name": "vscode-xwind",
  "version": "0.1.0",
  "publisher": "Arthie",
  "engines": {
    "vscode": "^1.44.0"
  },
  "author": "Arthie",
  "license": "MIT",
  "displayName": "xwind",
  "description": "VS Code extension for xwind",
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "babel-macros",
    "css-in-js",
    "css",
    "tailwind-ui",
    "tailwind-plugins",
    "babel-macro",
    "emotion",
    "babel",
    "autocomplete",
    "intellisense",
    "xwind"
  ],
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Arthie/vscode-xwind"
  },
  "bugs": {
    "url": "https://github.com/Arthie/vscode-xwind/issues"
  },
  "main": "./lib/extension",
  "contributes": {
    "configuration": {
      "title": "xwind",
      "properties": {
        "xwind.useCompletionItemProviderTriggerProxy": {
          "type": "boolean",
          "default": true,
          "description": "use Trigger character completion proxy."
        },
        "xwind.ignoreErrors": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "If this regex pattern string matches the error will be ignored"
        }
      }
    },
    "typescriptServerPlugins": [
      {
        "name": "typescript-xwind-plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "qna": "https://github.com/Arthie/vscode-tailwindcssinjs/issues",
  "dependencies": {
    "tailwindcss": "2.0.0-alpha.25-compat",
    "typescript": "^4.0.5",
    "typescript-xwind-plugin": "0.2.1"
  },
  "devDependencies": {
    "@types/vscode": "1.44.0"
  },
  "icon": "icon.png",
  "scripts": {
    "build": "tsc --build"
  }
}